{
  "name": "webpage-section-tracker",
  "version": "1.2.0",
  "description": "Web analytics/tracking library for browsers and bundlers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starlawfirm-org/webpage-section-tracker.git"
  },
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "build:dev": "tsup",
    "dev": "tsup --watch",
    "build:watch": "tsup --watch",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "analytics",
    "tracking",
    "web",
    "browser",
    "element-dwell",
    "viewport-tracking",
    "intersection-observer"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./cdn": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.iife.js"
    }
  },
  "author": "STARLAWFIRM Choi-Yeongung Software Development Manager",
  "license": "MIT",
  "unpkg": "dist/index.iife.js",
  "jsdelivr": "dist/index.iife.js",
  "browser": {
    "./dist/index.cjs": "./dist/index.mjs"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=20"
  },
  "bugs": {
    "url": "https://github.com/starlawfirm-org/webpage-section-tracker/issues"
  },
  "homepage": "https://github.com/starlawfirm-org/webpage-section-tracker#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^24.5.1",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}

